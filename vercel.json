{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/auth/login", "dest": "/dist/index.js" },
      { "src": "/auth/register", "dest": "/dist/index.js" },
      { "src": "/", "dest": "/dist/index.js" }
    ]
  }
  